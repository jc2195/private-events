<%= form_with model: @event do |f| %>
  <%= f.label :title, 'Title: '%>
  <%= f.text_field :title %><br>
  <%= f.label :description, 'Description: '%>
  <%= f.text_field :description %><br>
  <%= f.label :date, 'Date: '%>
  <%= f.date_field :date %><br>
  <%= f.label :location, 'Location: '%>
  <%= f.text_field :location %><br>
  <%= f.submit 'Post'%>
<% end %>